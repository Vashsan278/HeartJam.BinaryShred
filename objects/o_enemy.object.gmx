        }
    break;
    case states.chase :
        wait = 0;
        seen = true;
        if sprint = true {
            if x &lt; o_player.x-1 {
                x += o_player.robo_speed / 1.2;
            }
            if x &gt; o_player.x+1 {
                x -= o_player.robo_speed / 1.2;
            }
            if y &lt; o_player.y-1 {
                y += o_player.robo_speed / 1.2;
            }
            if y &gt; o_player.y+1 {
                y -= o_player.robo_speed / 1.2;
            }
