    case states.inactive :
        if seen = true {
            wait++;
        }
        
        if wait = 1 {
            audio_sound_gain(mus_danger, 0, 3000);
        }
        
        if wait = 90 {
            x = xstart;
            y = ystart;
            seen = false;
            wait = 0;
            sprint = true;
            if audio_is_playing(mus_danger) {
                audio_stop_sound(mus_danger);
                audio_play_sound(mus_overture,9,true);
